generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// TODO: Delete
model User {
  id     String @id @unique @default(uuid())
  userId String
  gender String
  sport  String
  meta   Meta?
}

model Meta {
  id        String @id @default(uuid())
  heartrate Int?
  watt      Int?
  speed     Int?
  User      User   @relation(fields: [userId], references: [id])
  userId    String @unique
}

